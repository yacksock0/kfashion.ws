        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionUserProfile" alias="KfashionUserProfile" />





        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionCategoryItem" alias="KfashionCategoryItem" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionUserGroupAdmin" alias="KfashionUserGroupAdmin" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionCategory" alias="KfashionCategory" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionEmailAuthority" alias="KfashionEmailAuthority" />
