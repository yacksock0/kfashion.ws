        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionUserGroupAuthority" alias="KfashionUserGroupAuthority" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionUserInfo" alias="KfashionUserInfo" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionWork" alias="KfashionWork" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionWorkHistory" alias="KfashionWorkHistory" />

